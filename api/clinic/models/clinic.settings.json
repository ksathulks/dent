{
  "kind": "collectionType",
  "connection": "default",
  "collectionName": "clinics",
  "info": {
    "name": "clinics"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": false
    },
    "address": {
      "type": "text"
    },
    "email": {
      "type": "email",
      "required": true,
      "unique": true
    },
    "phone": {
      "type": "biginteger",
      "required": true
    },
    "verificationStatus": {
      "type": "boolean"
    },
    "patients": {
      "collection": "patient"
    },
    "doctor_payments": {
      "collection": "doctor-payments"
    },
    "patient_payments": {
      "collection": "patient-payments"
    }
  }
}
